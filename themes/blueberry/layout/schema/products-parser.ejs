<%
	// Write schema.org product metedata

	breakArrayVariationsRecursive(productsList);

	productsList.forEach(product => {
		addPriceFromColorOption(product);
		setUniqueIdFromVariations(product);
		setExtraProductAttributes(product);
	});
%>

<% for (product of productsList){ %>
  <%- partial('schema/product', {product: product}) %>
<% } %>