    <!-- Global site tag (gtag.js) - Google Analytics & Ads -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-37279720-5"></script>
    <script>
      window.dataLayer = window.dataLayer || [];
      function gtag(){dataLayer.push(arguments);}
      gtag('js', new Date());

      gtag('config', 'UA-37279720-5');
      gtag('config', 'AW-780568854');
    </script>
    <!-- Event snippet for Buy button click conversion page
    In your html page, add the snippet and call gtag_report_conversion when someone clicks on the chosen link or button. -->
    <script>
      function gtag_report_conversion(url) {
        var callback = function () {
          if (typeof(url) != 'undefined') {
            window.location = url;
          }
        };
        gtag('event', 'conversion', {
            'send_to': 'AW-780568854/oPKzCOXL4YwBEJaSmvQC',
            'event_callback': callback
        });
        return false;
      }
    </script>

